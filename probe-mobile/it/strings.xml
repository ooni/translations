  <string name="General_AppName">OONI Probe</string>
  <string name="Onboarding_WhatIsOONIProbe_Title">Cos\'è OONI Probe?</string>
  <string name="Onboarding_WhatIsOONIProbe_Paragraph">La tua app per misurare la censura internet\n\nCi sono siti web e/o app social bloccati? La tua rete internet è stranamente lenta?\n\nScoprilo con OONI Probe!</string>
  <string name="Onboarding_WhatIsOONIProbe_GotIt">Ho capito</string>
  <string name="Onboarding_ThingsToKnow_Title">Sapevatelo!</string>
  <string name="Onboarding_ThingsToKnow_Bullet_1">I dati di OONI sono pubblici e i dettagli della tua rete saranno inclusi.</string>
  <string name="Onboarding_ThingsToKnow_Bullet_2">Chiunque stia monitorando la tua attività internet (es. provider o governo) potrà sapere che stai usando OONI Probe.</string>
  <string name="Onboarding_ThingsToKnow_Bullet_3">Potresti misurare siti web bloccati (ma puoi scegliere che siti testare).</string>
  <string name="Onboarding_ThingsToKnow_Button">Ho capito</string>
  <string name="Onboarding_ThingsToKnow_LearnMore">Per saperne di più</string>
  <string name="Onboarding_PopQuiz_Title">Quiz</string>
  <string name="Onboarding_PopQuiz_True">Vero</string>
  <string name="Onboarding_PopQuiz_False">Falso</string>
